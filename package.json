{
    "name": "regular-autocomplete",
    "displayName": "regular-autocomplete",
    "description": "the autocomplete of regular",
    "version": "0.1.0",
    "publisher": "chenshuyi",
    "engines": {
        "vscode": "^1.25.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chenshy39/regular-autocomplete.git"
    },
    "categories": [
        "Other"
    ],
    "author": {
        "name": "chenshuyi"
    },
    "icon": "image/icon.png",
    "activationEvents": [
        "onLanguage:html",
        "onLanguage:javascript"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
